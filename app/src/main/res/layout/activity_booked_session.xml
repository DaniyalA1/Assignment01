<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DEF8F8"
    tools:context=".BookedSessionActivity">

    <View
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="-50dp"
        android:alpha="0.7"
        android:background="@drawable/circle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginLeft="-50dp"
        android:alpha="0.7"
        android:background="@drawable/circle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="52dp"
        android:layout_height="48dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@android:color/transparent"
        android:onClick="onBackButtonClicked"
        android:src="@drawable/baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/text"
        android:layout_width="348dp"
        android:layout_height="35dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="-45dp"
        android:text="@string/booked"
        android:textColor="#157177"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/backButton"
        />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView1"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cardCornerRadius="20dp"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintStart_toStartOf="@id/backButton"
        app:layout_constraintTop_toBottomOf="@id/backButton" >

        <ImageView
            android:id="@+id/imageVie1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/aiden_walker"
            />
    </androidx.cardview.widget.CardView>


    <TextView
        android:id="@+id/textbox1"
        android:layout_width="250dp"
        android:layout_height="55dp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="190dp"
        android:background="@drawable/white_rounded_box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textbox11"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_marginTop="198dp"
        android:layout_marginLeft="-10dp"
        android:background="@android:color/transparent"
        android:text="@string/john"
        android:textColor="#157177"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="218dp"
        android:background="@android:color/transparent"
        android:text="UX Designer @ Google"
        android:textSize="8.8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/textbox13"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="270dp"
        android:background="@android:color/transparent"
        android:text="@string/date"
        android:textColor="#ECB906"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:cardCornerRadius="20dp"
        android:layout_marginTop="170dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintStart_toStartOf="@id/backButton"
        app:layout_constraintTop_toBottomOf="@id/backButton" >

        <ImageView
            android:id="@+id/imageVie2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/ava_thompson"
            />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textbox2"
        android:layout_width="250dp"
        android:layout_height="55dp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="320dp"
        android:background="@drawable/white_rounded_box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textbox21"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_marginTop="328dp"
        android:layout_marginLeft="-10dp"
        android:background="@android:color/transparent"
        android:text="@string/Emma"
        android:textColor="#157177"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="348dp"
        android:background="@android:color/transparent"
        android:text="Android Developer"
        android:textSize="8.8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/textbox23"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginTop="330dp"
        android:layout_marginLeft="270dp"
        android:background="@android:color/transparent"
        android:text="@string/date2"
        android:textColor="#ECB906"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>