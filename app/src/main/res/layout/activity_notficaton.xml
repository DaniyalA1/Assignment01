<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DEF8F8"
    tools:context=".NotficatonActivity">

    <View
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="-50dp"
        android:alpha="0.7"
        android:background="@drawable/circle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginLeft="-50dp"
        android:alpha="0.7"
        android:background="@drawable/circle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="52dp"
        android:layout_height="48dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@android:color/transparent"
        android:onClick="onBackButtonClicked"
        android:src="@drawable/baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/text"
        android:layout_width="348dp"
        android:layout_height="35dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="-45dp"
        android:text="@string/notify"
        android:textColor="#157177"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/backButton"
       />

    <ImageView
        android:id="@+id/image"
        android:layout_width="48dp"
        android:layout_height="35dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="-38dp"
        android:src="@drawable/notify"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/backButton"
        />

    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="320dp"
        android:layout_marginTop="180dp"
        android:text="@string/clear"
        android:textSize="17.5dp"
        android:textColor="#ECB800"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="220dp"
        android:text="You have added a mentor, Alex. Thank you!"
        android:textSize="15.5dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/crossButton1"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_marginLeft="360dp"
        android:layout_marginTop="225dp"
        android:background="@android:color/transparent"
        android:src="@drawable/close"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="16dp"
        android:alpha="0.7"
        android:background="#B6ECE7"
        app:layout_constraintTop_toBottomOf="@id/crossButton1"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="270dp"
        android:text="Your favorites list is expanding. Impressive"
        android:textSize="15.5dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/crossButton2"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_marginLeft="360dp"
        android:layout_marginTop="275dp"
        android:background="@android:color/transparent"
        android:src="@drawable/close"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="66dp"
        android:alpha="0.7"
        android:background="#B6ECE7"
        app:layout_constraintTop_toBottomOf="@id/crossButton1"
         />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="320dp"
        android:text="You have a new message in community."
        android:textSize="15.5dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/crossButton3"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_marginLeft="360dp"
        android:layout_marginTop="325dp"
        android:background="@android:color/transparent"
        android:src="@drawable/close"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="116dp"
        android:alpha="0.7"
        android:background="#B6ECE7"
        app:layout_constraintTop_toBottomOf="@id/crossButton1"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="370dp"
        android:text="@string/text12"
        android:textSize="15.5dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/crossButton4"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_marginLeft="360dp"
        android:layout_marginTop="375dp"
        android:background="@android:color/transparent"
        android:src="@drawable/close"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line4"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="166dp"
        android:alpha="0.7"
        android:background="#B6ECE7"
        app:layout_constraintTop_toBottomOf="@id/crossButton1"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="420dp"
        android:text="Thank you for booking a session. Good Luck."
        android:textSize="15.5dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/crossButton5"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_marginLeft="360dp"
        android:layout_marginTop="425dp"
        android:background="@android:color/transparent"
        android:src="@drawable/close"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line5"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="216dp"
        android:alpha="0.7"
        android:background="#B6ECE7"
        app:layout_constraintTop_toBottomOf="@id/crossButton1"
        />



</androidx.constraintlayout.widget.ConstraintLayout>